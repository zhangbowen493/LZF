<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue"
    android:fitsSystemWindows="true" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white" >

        <include
            android:id="@+id/act_top"
            layout="@layout/activity_title" />

        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/act_top"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="40dp" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:background="@drawable/login_icon_success"
                android:contentDescription="@string/img" />

            <TextView
                android:id="@+id/activity_regist_success_tv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/imageView1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="6dp"
                android:text="@string/Transfer_success"
                android:textColor="@color/orange_normal"
                android:textSize="22sp"
                android:textStyle="bold" />
        </RelativeLayout>

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/relativeLayout1"
            android:layout_marginTop="30dp"
            android:background="#efefef" />

        <LinearLayout
            android:id="@+id/Linear_out1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/edittext_high"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/view1"
            android:background="@color/gray_bg"
            android:orientation="horizontal" >

            <View
                android:id="@+id/view7"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="1dp"
                android:layout_weight="4"
                android:background="#efefef" />

            <TextView
                android:id="@+id/text_mingxi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="充值明细"
                android:textColor="#eaeaea"
                android:textSize="@dimen/tv_size_low" />

            <View
                android:id="@+id/view8"
                android:layout_width="0dp"
                android:layout_height="1dp"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="15dp"
                android:layout_weight="4"
                android:background="#efefef" />
        </LinearLayout>

        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#efefef" />

        <LinearLayout
            android:id="@+id/Linear_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/Linear_out1"
            android:background="@color/white"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_box_bg"
                android:orientation="horizontal"
                android:paddingBottom="15dp"
                android:paddingTop="15dp" >

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/widget_ml"
                    android:contentDescription="@string/img"
                    android:src="@drawable/icon_money" />

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/act_edt_add_card_two_check_code"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:gravity="center_vertical|left"
                        android:inputType="number"
                        android:text="转账金额"
                        android:textColor="#44adfe"
                        android:textSize="@dimen/tv_size_s" >
                    </TextView>
                    <!-- android:hint="请输入金额/分" -->

                    <TextView
                        android:id="@+id/act_money_text"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:layout_marginRight="@dimen/widget_ml"
                        android:gravity="center|right"
                        android:text="123"
                        android:textColor="#44adfe"
                        android:textSize="@dimen/tv_size_s" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/Linear_out1"
            android:background="#efefef" />

        <LinearLayout
            android:id="@+id/Linear_layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/Linear_layout1"
            android:background="@color/white"
            android:orientation="vertical" >

            <View
                android:id="@+id/view6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@id/Linear_layout1"
                android:background="#efefef" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_box_bg"
                android:orientation="horizontal"
                android:paddingBottom="15dp"
                android:paddingTop="15dp" >

                <ImageView
                    android:id="@+id/imageView16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/widget_ml"
                    android:contentDescription="@string/img"
                    android:src="@drawable/icon_zhanghu" />

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/act_edt_add_card_two_check_number"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:gravity="center_vertical|left"
                        android:inputType="number"
                        android:text="转出帐户"
                        android:textColor="#666666"
                        android:textSize="@dimen/tv_size_s" >

                        <requestFocus />
                    </TextView>

                    <TextView
                        android:id="@+id/act_number_text"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:layout_marginRight="@dimen/widget_ml"
                        android:gravity="center|right"
                        android:text="123"
                        android:textColor="#333333"
                        android:textSize="@dimen/tv_size_s" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/Linear_layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/Linear_layout2"
            android:background="@color/white"
            android:orientation="vertical" >

            <View
                android:id="@+id/view61"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@id/Linear_layout1"
                android:background="#efefef" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_box_bg"
                android:orientation="horizontal"
                android:paddingBottom="15dp"
                android:paddingTop="15dp" >

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/widget_ml"
                    android:contentDescription="@string/img"
                    android:src="@drawable/icon_chongzhifangshi" />

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/act_edt_add_card_two_check_mode"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:gravity="center_vertical|left"
                        android:inputType="number"
                        android:text="转入帐户"
                        android:textColor="#666666"
                        android:textSize="@dimen/tv_size_s" >

                        <requestFocus />
                    </TextView>

                    <TextView
                        android:id="@+id/act_mode_text"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:layout_marginRight="@dimen/widget_ml"
                        android:gravity="center|right"
                        android:text="123"
                        android:textColor="#333333"
                        android:textSize="@dimen/tv_size_s" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/Linear_layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/Linear_layout3"
            android:background="@color/white"
            android:orientation="vertical" >

            <View
                android:id="@+id/view62"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@id/Linear_layout1"
                android:background="#efefef" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_box_bg"
                android:orientation="horizontal"
                android:paddingBottom="15dp"
                android:paddingTop="15dp" >

                <ImageView
                    android:id="@+id/imageView18"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/widget_ml"
                    android:contentDescription="@string/img"
                    android:src="@drawable/icon_time" />

                <LinearLayout
                    android:layout_width="0.0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/act_edt_add_card_two_check_time"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:gravity="center_vertical|left"
                        android:inputType="number"
                        android:text="交易时间"
                        android:textColor="#666666"
                        android:textSize="@dimen/tv_size_s" >

                        <requestFocus />
                    </TextView>

                    <TextView
                        android:id="@+id/act_time_text"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/widget_ml"
                        android:layout_marginRight="@dimen/widget_ml"
                        android:gravity="center|right"
                        android:text="123"
                        android:textColor="#333333"
                        android:textSize="@dimen/tv_size_s" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/Linear_layout5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/Linear_layout4"
            android:background="@color/gray_bg"
            android:orientation="vertical" >
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>