<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white" >
    <include
        android:id="@+id/act_top"
        layout="@layout/activity_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ll_transaction_detail_top_hight"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/act_iv_transaction_detail2_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:src="@drawable/icon_money" />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/cyan_tv"
            android:textSize="@dimen/font_size_15"
            android:layout_marginLeft="24dp"
            android:text="+0.15" />
        <View 
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            />
        
        <TextView
            android:id="@+id/act_tv_transaction_detail2_state"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/cyan_tv"
            android:textSize="@dimen/font_size_15"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="17dp"
            android:text="充值成功" />

    </LinearLayout>
    
    <View 
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:background="@color/gray_rl_act"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ll_transaction_detail_list_hight"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="16dp"
            android:src="@drawable/icon_chongzhizhanghu" />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_account_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_trsd"
            android:text="充值账户" />
        <View 
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_account"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_item_trsr"
            android:layout_marginRight="17dp"
            android:text="18510000000" />

    </LinearLayout>
     <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_rl_act"
        />
     
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ll_transaction_detail_list_hight"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/act_iv_transaction_detail2_way_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="13dp"
            android:src="@drawable/icon_way" />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_way_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_trsd"
            android:text="充值方式" />
        <View 
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_way"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_item_trsr"
            android:layout_marginRight="17dp"
            android:text="中国银行-储蓄卡（7399）" />

    </LinearLayout>
     <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_rl_act"
        />
     
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/ll_transaction_detail_list_hight"
        android:gravity="center_vertical" >

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="14dp"
            android:layout_marginRight="13dp"
            android:src="@drawable/icon_time" />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_time_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_trsd"
            android:text="交易时间" />
        <View 
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"
            />

        <TextView
            android:id="@+id/act_tv_transaction_detail2_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/tv_size_low"
            android:textColor="@color/black_tv_item_trsr"
            android:layout_marginRight="17dp"
            android:text="2016-3-30 10:33:43" />

    </LinearLayout>
     <View 
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray_rl_act"
        />

</LinearLayout>
