<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <!-- spinner -->
    <style name="Theme.CustomDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/icon_login_9</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="PopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="MyButton">
        <item name="android:background">@drawable/button_select_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:height">@dimen/button_high</item>
        <item name="android:textSize">@dimen/tv_size</item>
        <item name="android:paddingLeft">@dimen/button_tv_padding</item>
        <item name="android:paddingRight">@dimen/button_tv_padding</item>
        <item name="android:paddingTop">@dimen/button_tv_padding</item>
        <item name="android:paddingBottom">@dimen/button_tv_padding</item>
        <item name="android:layout_marginLeft">@dimen/widget_ml_big</item>
        <item name="android:layout_marginRight">@dimen/widget_ml_big</item>
    </style>

    <style name="MyButtonOrange">
        <item name="android:background">@drawable/button_orange_select_bg</item>
        <item name="android:height">@dimen/button_high</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/tv_size</item>
        <item name="android:paddingLeft">@dimen/button_tv_padding</item>
        <item name="android:paddingRight">@dimen/button_tv_padding</item>
        <item name="android:paddingTop">@dimen/button_tv_padding</item>
        <item name="android:paddingBottom">@dimen/button_tv_padding</item>
    </style>

    <style name="MyButtonOrange_small">
        <item name="android:background">@drawable/button_orange_select_bg</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/tv_size</item>
        <item name="android:paddingTop">@dimen/button_tv_padding_small</item>
        <item name="android:paddingBottom">@dimen/button_tv_padding_small</item>
    </style>

    <style name="MyWithdrawalButtonStyle">
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@drawable/withdrawal_button_tv_select_color</item>
        <item name="android:textSize">@dimen/tv_size</item>
    </style>

    <style name="MyWithdrawalOrangeButtonStyle">
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@drawable/withdrawal_orange_button_tv_select_color</item>
        <item name="android:textSize">@dimen/tv_size</item>
    </style>

    <style name="myCBstyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/mycheck_selection</item>
        <item name="android:paddingLeft">5.0dip</item>
    </style>

    <style name="main_popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/main_setting_popwindow_start</item>
        <item name="android:windowExitAnimation">@anim/main_setting_popwindow_over</item>
    </style>

    <style name="passwordDilog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="myDilog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 是否模糊 -->
    </style>

    <style name="mystyle" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="@android:windowExitAnimation">@anim/dialog_exit</item>
    </style>

    <style name="common_title_IV_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dip</item>
        <item name="android:background">@drawable/ic_back</item>
    </style>

    <style name="common_title_TV_center">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="common_title_TV_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/ic_but_bg</item>
        <item name="android:layout_marginRight">7dp</item>
    </style>

    <style name="KeyBoardAnime">
        <item name="android:windowEnterAnimation">@anim/flyin</item>
        <item name="android:windowExitAnimation">@anim/flyout</item>
    </style>

     <style name="AnimSelectBank" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/banklist_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/banklist_push_bottom_out</item>
    </style>
    
    <style name="MyDialogStyleBottom" parent="android:Theme.Light">
        <item name="android:windowAnimationStyle">@style/AnimSelectBank</item>
        <item name="android:windowFrame">@null</item>
;!-- 边框 -->          <item name="android:windowIsFloating">true</item>
;!-- 是否浮现在activity之上 -->          <item name="android:windowIsTranslucent">true</item>
;!-- 半透明 -->          <item name="android:windowNoTitle">true</item>
;!-- 无标题 -->          <item name="android:windowBackground">@android:color/transparent</item>
;!-- 背景透明 -->        <item name="android:backgroundDimEnabled">true</item>
;!-- 模糊 -->  </style>

</resources>